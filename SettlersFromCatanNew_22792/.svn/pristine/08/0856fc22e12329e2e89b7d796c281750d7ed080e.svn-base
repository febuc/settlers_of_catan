package networking.MessageObjects;

import com.google.gson.annotations.SerializedName;

/**
 * Class representing the objects used for sending messages from one player to the other ones (through server)
 * @author Marcelina
 */
public class SendChat {
	@SerializedName("Nachricht") private String message;

	/**
	 * Constructor
	 * @param message The chat message to send to the server.
	 */
	public SendChat(String message) {
		setMessaage(message);
	}
	//Getters and Setters

	public String getMessage() {
		return message;
	}

	public void setMessaage(String message) {
		this.message = message;
	}
	
	
}
