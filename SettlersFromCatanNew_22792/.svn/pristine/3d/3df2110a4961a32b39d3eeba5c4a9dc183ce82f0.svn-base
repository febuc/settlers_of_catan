package tools;

/**
 * Lists all kinds of port types
 *
 * @author Felip
 */
public enum PortTypes {
    NONE, THREE_FOR_ONE, TWO_WOOD_FOR_ONE, TWO_WOOL_FOR_ONE, TWO_STONE_FOR_ONE, TWO_GRAIN_FOR_ONE, TWO_LOAM_FOR_ONE;

    @Override
    public String toString() {
        switch (this) {

            case THREE_FOR_ONE:
                return "3 : 1";
            case TWO_WOOD_FOR_ONE:
                return "2 Wood :1";
            case TWO_WOOL_FOR_ONE:
                return "2 Wool : 1";
            case TWO_STONE_FOR_ONE:
                return "2 Stone : 1";
            case TWO_GRAIN_FOR_ONE:
                return "2 Grain : 1";
            case TWO_LOAM_FOR_ONE:
                return "2 Loam : 1";
            default:
                return "4:1";
        }
    }
}
