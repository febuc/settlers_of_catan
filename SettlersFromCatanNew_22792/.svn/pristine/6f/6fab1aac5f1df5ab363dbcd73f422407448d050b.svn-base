#MenuButton {
    -fx-background-color: 
        #090a0c,
        linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%),
        linear-gradient(#20262b, #191d22),
        radial-gradient(center 50% 0%, radius 100%, rgba(114,131,148,0.9), rgba(255,255,255,0));
    -fx-background-radius: 5,4,3,5;
    -fx-background-insets: 0,1,2,0;
    -fx-text-fill: white;
    -fx-text-fill: linear-gradient(white, #d0d0d0);
    -fx-padding: 10 20 10 20;
    -fx-shape: "M0 0 L0 50 L25 45 L50 50 L50 0 Z";
    
}

#GameOverButton {
	-fx-background-color: 
        rgb(59,58,54),
        linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%),
        linear-gradient(#20262b, #191d22),
        radial-gradient(center 50% 0%, radius 100%, rgba(59,58,54,0.9), rgba(255,255,255,0));
    -fx-background-radius: 5,4,3,5;
    -fx-background-insets: 0,1,2,0;
    -fx-text-fill: rgb(92,61,56);
    -fx-text-fill: linear-gradient(rgb(92,61,56), rgb(200,169,164));
    -fx-padding: 10 20 10 20;
    -fx-shape: "M0 0 L0 50 L25 45 L50 50 L50 0 Z";
    
}


#VictoryButton {
	-fx-background-color: 
        rgb(59,58,54),
        linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%),
        linear-gradient(#20262b, #191d22),
        radial-gradient(center 50% 0%, radius 100%, rgba(59,58,54,0.9), rgba(255,255,255,0));
    -fx-background-radius: 5,4,3,5;
    -fx-background-insets: 0,1,2,0;
    -fx-text-fill: rgb(92,61,56);
    -fx-text-fill: linear-gradient(rgb(92,61,56), rgb(200,169,164));
    -fx-padding: 10 20 10 20;
    -fx-shape: "M0 0 L0 50 L25 45 L50 50 L50 0 Z";
    
}
#MenuButton:hover {
    -fx-background-insets: 0,20,4,0;   
     -fx-text-fill: gray;    
    -fx-shape: "M0 0 L0 50 L25 60 L50 50 L50 0 Z";
}

#GameOverButton:hover {
	 -fx-background-insets: 0,20,4,0;   
     -fx-text-fill:  rgb(92,61,56);    
     -fx-text-fill: linear-gradient(rgb(92,61,56), rgb(200,169,164));
    -fx-shape: "M0 0 L0 50 L25 60 L50 50 L50 0 Z";
}

#VictoryButton:hover {
	 -fx-background-insets: 0,20,4,0;   
     -fx-text-fill:  rgb(92,61,56);    
     -fx-text-fill: linear-gradient(rgb(92,61,56), rgb(200,169,164));
    -fx-shape: "M0 0 L0 50 L25 60 L50 50 L50 0 Z";
}

#MenuButton:pressed, #GameOverButton:pressed, VictoryButton:pressed {
    -fx-shape: "M0 0 L0 50 L25 80 L50 50 L50 0 Z";
}

#MenuButton2 {
    -fx-background-color: 
        #090a0c,
        linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%),
        linear-gradient(#20262b, #191d22),
        radial-gradient(center 50% 0%, radius 100%, rgba(114,131,148,0.9), rgba(255,255,255,0));
    -fx-background-radius: 5,4,3,5;
    -fx-background-insets: 0,1,2,0;
    -fx-padding: 10 20 10 20;
    -fx-shape: "M0 10 L0 50 L25 55 L50 50 L50 10 Z";
    
}
#MenuButton2:hover {
    -fx-background-insets: 0,20,4,0;   
     -fx-text-fill: gray;    
    -fx-shape: "M0 10 L0 50 L25 60 L50 50 L50 10 Z";
     

}
#MenuButton2:pressed {
    -fx-shape: "M0 0 L0 50 L25 80 L50 50 L50 0 Z";
}
#MenuButton2 Text {
    -fx-effect: dropshadow( one-pass-box , rgba(0,0,0,0.9) , 1, 0.0 , 0 , 1 );
}

#chipNumber {
    -fx-background-color: 
        linear-gradient(#686868 0%, #232723 25%, #373837 75%, #757575 100%),
        linear-gradient(#020b02, #3a3a3a),
        linear-gradient(#b9b9b9 0%, #c2c2c2 20%, #afafaf 80%, #c8c8c8 100%),
        linear-gradient(#f5f5f5 0%, #dbdbdb 50%, #cacaca 51%, #d7d7d7 100%);
    -fx-background-insets: 0,1,2,3;
    -fx-background-radius: 25em;    
    -fx-effect: dropshadow( three-pass-box , rgba(255,255,255,0.2) , 1, 0.0 , 0 , 1);

}
#avatar {
    -fx-background-color: 
        linear-gradient(#686868 0%, #232723 25%, #373837 75%, #757575 100%),
        linear-gradient(#020b02, #3a3a3a),
        linear-gradient(#b9b9b9 0%, #c2c2c2 20%, #afafaf 80%, #c8c8c8 100%),
        linear-gradient(#f5f5f5 0%, #dbdbdb 50%, #cacaca 51%, #d7d7d7 100%);
    -fx-background-insets: 0,1,2,3;
    -fx-background-radius: 25em;    

}
#avatar:hover {
    -fx-background-insets: 3,3,5,6;

}
#avatar:pressed {
    -fx-background-insets: 5,5,7,8;
}
#roundedWindow {
    -fx-background-color: #3b3d3f;
    -fx-background-insets: 0,1,2,3;
    -fx-background-radius: 7em;    

}
#roundedWindow:hover {
    -fx-background-insets: 3,3,5,6;
}
#roundedWindow:pressed {
    -fx-background-insets: 5,5,7,8;
}
#port {
    -fx-background-color: 
        rgb(195, 196, 196,0.4),
        linear-gradient(#d6d6d6 50%, white 40%),
        radial-gradient(center 50% -40%, radius 200%, #e6e6e6 45%, rgba(230,230,230,0) 50%);
    -fx-background-radius: 30;
    -fx-background-insets: 0,1,1;
    -fx-text-fill: black;
    -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 3, 0.0 , 0 , 1 );
}
#notification {
    -fx-background-color: 
        #090a0c,
        linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%),
        linear-gradient(#20262b, #191d22),
        radial-gradient(center 50% 0%, radius 100%, rgba(114,131,148,0.9), rgba(255,255,255,0));
    -fx-background-radius: 5,4,3,5;
    -fx-background-insets: 3,5,7,3;
    -fx-text-fill: white;
    -fx-text-fill: linear-gradient(white, #d0d0d0);
    -fx-padding: 10 20 10 20;
}
.slider .track {
      -fx-background-color:
          -fx-shadow-highlight-color,
          linear-gradient(to bottom, derive(-fx-text-box-border, -10%), -fx-text-box-border),
          linear-gradient(to bottom,
            derive(-fx-control-inner-background, -9%),
            derive(-fx-control-inner-background, 0%),
            derive(-fx-control-inner-background, -5%),
            derive(-fx-control-inner-background, -12%)
          );
    -fx-background-insets: 0 0 -1 0, 0, 1;
    -fx-background-radius: 0.5em, 0.5em, 0.366667em; /* 3 3 2 */
    -fx-padding: 0.6em; /* 3 */
    -fx-control-inner-background: gray;
}

#outlineText {
    -fx-stroke: black;
    -fx-stroke-width: 2;
}