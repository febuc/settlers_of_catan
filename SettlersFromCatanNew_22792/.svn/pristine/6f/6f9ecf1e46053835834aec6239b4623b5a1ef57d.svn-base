package networking.MessageObjects;

import com.google.gson.annotations.SerializedName;

/**
 * Class representing the server responses that will be send from server to
 * client to confirm their actions.
 * 
 * @author Marcelina
 */
public class ServerResponse {
	@SerializedName("Serverantwort")
	private String serverResponse;

	/**
	 * constructor
	 * 
	 * @param serverResponse
	 *            the message to be conveyed to the client
	 */
	public ServerResponse(String serverResponse) {
		this.serverResponse = serverResponse;
	}

	public String getServerResponse() {
		return serverResponse;
	}
}
